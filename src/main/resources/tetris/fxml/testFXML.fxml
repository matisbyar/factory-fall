<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tetris.TestFXML">
   <children>
      <VBox alignment="CENTER" layoutX="200.0" layoutY="150.0" prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label fx:id="label" text="0" />
            <Button mnemonicParsing="false" onAction="#bonjour" text="Bonjour">
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin></Button>
            <Button mnemonicParsing="false" onMouseClicked="#switchScene2" text="scene 2">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
